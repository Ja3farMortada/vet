{
    "name": "petshop",
    "version": "1.5.1",
    "description": "Pet Shop Veterinary System",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "server": "nodemon temporary.js",
        "package": "electron-packager .",
        "pack": "electron-builder --dir",
        "build": "electron-builder --x64",
        "publish": "electron-builder --x64 -p always"
    },
    "author": "Jaafar Mortada",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Ja3farMortada/vet.git"
    },
    "build": {
        "appId": "com.binariez.Ja3farMortada.vet",
        "productName": "PetShop Veterinary",
        "target": "NSIS",
        "publish": [
            {
                "provider": "github",
                "owner": "Ja3farMortada",
                "repo": "vet"
            }
        ],
        "directories": {
            "buildResources": "resources"
        }
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.14.0",
        "@sweetalert2/themes": "^5.0.5",
        "address": "^1.2.0",
        "angular": "^1.8.0",
        "angular-animate": "^1.8.0",
        "angular-aria": "^1.8.0",
        "angular-material": "^1.1.20",
        "angular-messages": "^1.8.0",
        "angular-route": "^1.8.0",
        "angular-utils-pagination": "^0.11.1",
        "animate.css": "^4.1.1",
        "bootstrap": "^4.5.2",
        "chart.js": "^3.6.0",
        "cors": "^2.8.5",
        "electron-context-menu": "^3.1.1",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.2.4",
        "electron-updater": "^4.3.4",
        "express": "^4.17.1",
        "jquery": "^3.5.1",
        "jquery-ui": "^1.12.1",
        "jsonwebtoken": "^8.5.1",
        "md5": "^2.2.1",
        "moment": "^2.27.0",
        "mysql2": "^2.3.2",
        "mysqldump": "^3.2.0",
        "node-machine-id": "^1.1.12",
        "popper.js": "^1.16.1",
        "sweetalert2": "^11.1.8",
        "update-electron-app": "^1.5.0"
    },
    "devDependencies": {
        "electron": "^10.4.7",
        "electron-builder": "^22.11.1",
        "electron-packager": "^15.1.0",
        "nodemon": "^2.0.4"
    }
}
